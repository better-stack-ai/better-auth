{
  "name": "@btst/adapter-mongodb",
  "version": "1.0.2",
  "description": "MongoDB adapter for btst",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/olliethedev/better-auth/tree/main/packages/btst#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/olliethedev/better-auth",
    "directory": "packages/btst/adapter-mongodb"
  },
  "keywords": [
    "mongodb",
    "database",
    "adapter",
    "typescript",
    "btst"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild --clean",
    "stub": "unbuild --stub",
    "typecheck": "tsc --project tsconfig.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@btst/db": "workspace:*"
  },
  "peerDependencies": {
    "better-auth": ">=1.0.0",
    "mongodb": "^6.0.0"
  },
  "devDependencies": {
    "better-auth": "workspace:*",
    "typescript": "catalog:",
    "unbuild": "catalog:"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
